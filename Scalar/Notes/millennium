set tmp = /tmp/1
set out = millennium_data.out
set periods = 1000
set t1 = ( \
"150   155   160   165   170   175   180   185   190   195   200   205" \
"210   215   220   225   230   235   240   245   250   255   260   265" \
"270   275   280   285   290   295   300   305   310   315   320   325" \
"330   335   340   345   350   355   360   365   370   375   380   385" \
"390   395   400   405   410   415   420   425   430   435   440   445" \
"450   455   460   465   470   475   480   485   490   495   500" \
"505   510   515   520   525   530   535   540   545   550   555   560" \
"565   570   575   580   585   590   595   600   605   610   615   620" \
"625   630   635   640   645   650   655   660   665   670   675   680" \
"685   690   695   700   720         740" \
"760         780         800         820         840         860" \
"880         900         920         940         960         980" \
"1000        1040        1080        1120        1160        1200" \
"1240        1280        1320        1360        1400        1440" \
"1480        1520        1560        1600        1640        1680" \
"1720        1760        1800        1840        1880        1920" \
"1960        2000" )
rm $out
foreach period__g ( $t1 )
 echo ' '
 ../Kepler_TEST $period__g $periods | tee /dev/tty | \
 grep -e "^ *[0-9]" > /tmp/1
 take /tmp/1 3 6 7 >> $out
 echo period = $period__g done.
end
